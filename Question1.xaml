<Window x:Class="CSharp.Net_Quiz3.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CSharp.Net_Quiz3"
        mc:Ignorable="d"
        Title="Question 1" Height="250" Width="400">
    <StackPanel Margin="5,5,5,5">
        <Label Content="Student ID"></Label>
        <TextBox x:Name="sID"></TextBox>
        <Label Content="First Name"></Label>
        <TextBox x:Name="fName"></TextBox>
        <Label Content="Last Name"></Label>
        <TextBox x:Name="lName"></TextBox>
        <Label>Is Registered?</Label>
        <CheckBox x:Name="iReg"></CheckBox>
        <Separator></Separator>
        <Button Content="Save" Click="Save_Click"></Button>
    </StackPanel>
</Window>
