<Window x:Class="CSharp.Net_Quiz3.Question2"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CSharp.Net_Quiz3"
        mc:Ignorable="d"
        Title="Question 2" Height="250" Width="280"
        Loaded="Window_Loaded">
    <Grid Margin="10">
        <DataGrid Name="sList" AutoGenerateColumns="False">
            <DataGrid.Columns>
                <DataGridTextColumn Header="ID" Binding="{Binding Id}" />
                <DataGridTextColumn Header="First Name" Binding="{Binding FirstName}" />
                <DataGridTextColumn Header="Last Name" Binding="{Binding LastName}" />
                <DataGridTextColumn Header="Registered" Binding="{Binding isRegistered}" />
            </DataGrid.Columns>
        </DataGrid>
    </Grid>
</Window>
